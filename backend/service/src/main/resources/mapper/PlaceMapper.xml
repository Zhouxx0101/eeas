<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.eeas.mapper.PlaceMapper">


    <select id="listPlace" parameterType="String" resultType="String">
        select place from eeas_place;
    </select>

    <select id="getPlacesWithID" parameterType="Integer" resultType="String">
        select place from eeas_place where id=#{id}
    </select>
</mapper>