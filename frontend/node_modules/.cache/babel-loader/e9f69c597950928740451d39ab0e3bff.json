isRelogin.show = true\r\n        // 判断当前用户是否已拉取完user_info信息\r\n        store.dispatch('GetInfo').then(() => {\r\n          isRelogin.show = false\r\n          store.dispatch('GenerateRoutes').then(accessRoutes => {\r\n            // 根据roles权限生成可访问的路由表\r\n            router.addRoutes(accessRoutes) // 动态添加可访问路由表\r\n            next({ ...to, replace: true }) // hack方法 确保addRoutes已完成\r\n          })\r\n        }).catch(err => {\r\n            store.dispatch('LogOut').then(() => {\r\n              Message.error(err)\r\n              next({ path: '/' })\r\n            })\r\n          })\r\n      } else {\r\n        next()\r\n      }\r\n    }\r\n  } else {\r\n    // 没有token\r\n    if (whiteList.indexOf(to.path) !== -1) {\r\n      // 在免登录白名单，直接进入\r\n      next()\r\n    } else {\r\n      next(`/login?redirect=${to.fullPath}`) // 否则全部重定向到登录页\r\n      NProgress.done()\r\n    }\r\n  }\r\n})\r\n\r\nrouter.afterEach(() => {\r\n  NProgress.done()\r\n})\r\n"],"mappings":";;;;;;AAAA;;AACA;;AACA;;AACA;;AACA;;AACA;;AACA;;AAEAA,kBAAA,CAAUC,SAAV,CAAoB;EAAEC,WAAW,EAAE;AAAf,CAApB;;AAEA,IAAMC,SAAS,GAAG,CAAC,QAAD,EAAW,gBAAX,EAA6B,OAA7B,EAAsC,WAAtC,CAAlB;;AAEAC,eAAA,CAAOC,UAAP,CAAkB,UAACC,EAAD,EAAKC,IAAL,EAAWC,IAAX,EAAoB;EACpCR,kBAAA,CAAUS,KAAV;;EACA,IAAI,IAAAC,cAAA,GAAJ,EAAgB;IACdJ,EAAE,CAACK,IAAH,CAAQC,KAAR,IAAiBC,cAAA,CAAMC,QAAN,CAAe,mBAAf,EAAoCR,EAAE,CAACK,IAAH,CAAQC,KAA5C,CAAjB;IACA;;IACA,IAAIN,EAAE,CAACS,IAAH,KAAY,QAAhB,EAA0B;MACxBP,IAAI,CAAC;QAAEO,IAAI,EAAE;MAAR,CAAD,CAAJ;;MACAf,kBAAA,CAAUgB,IAAV;IACD,CAHD,MAGO;MACL,IAAIH,cAAA,CAAMI,OAAN,CAAcC,KAAd,CAAoBC,MAApB,KAA+B,CAAnC,EAAsC;QACpCC,kBAAA,CAAUC,IAAV,GAAiB,IAAjB,CADoC,CAEpC;;QACAR,cAAA,CAAMC,QAAN,CAAe,SAAf,EAA0BQ,IAA1B,CAA+B,YAAM;UACnCF,kBAAA,CAAUC,IAAV,GAAiB,KAAjB;;UACAR,cAAA,CAAMC,QAAN,CAAe,gBAAf,EAAiCQ,IAAjC,CAAsC,UAAAC,YAAY,EAAI;YACpD;YACAnB,eAAA,CAAOoB,SAAP,CAAiBD,YAAjB,EAFoD,CAErB;;;YAC/Bf,IAAI,6DAAMF,EAAN;cAAUmB,OAAO,EAAE;YAAnB,GAAJ,CAHoD,CAGrB;UAChC,CAJD;QAKD,CAPD,EAOGC,KAPH,CAOS,UAAAC,GAAG,EAAI;UACZd,cAAA,CAAMC,QAAN,CAAe,QA