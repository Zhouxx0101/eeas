 size=\"small\" :inline=\"true\" v-show=\"showSearch\" label-width=\"68px\">\r\n      <el-form-item label=\"患者id\" prop=\"pid\">\r\n        <el-input\r\n          v-model=\"queryParams.pid\"\r\n          placeholder=\"请输入患者id\"\r\n          clearable\r\n          @keyup.enter.native=\"handleQuery\"\r\n        />\r\n      </el-form-item>\r\n      <el-form-item label=\"起始时间\" prop=\"startTime\">\r\n        <el-date-picker clearable\r\n          v-model=\"queryParams.startTime\"\r\n          type=\"date\"\r\n          value-format=\"yyyy-MM-dd\"\r\n          placeholder=\"请选择起始时间\">\r\n        </el-date-picker>\r\n      </el-form-item>\r\n      <el-form-item label=\"结束时间\" prop=\"endTime\">\r\n        <el-date-picker clearable\r\n          v-model=\"queryParams.endTime\"\r\n          type=\"date\"\r\n          value-format=\"yyyy-MM-dd\"\r\n          placeholder=\"请选择结束时间\">\r\n        </el-date-picker>\r\n      </el-form-item>\r\n      <el-form-item label=\"场所\" prop=\"place\">\r\n        <el-input\r\n          v-model=\"queryParams.place\"\r\n          placeholder=\"请输入场所\"\r\n          clearable\r\n          @keyup.enter.native=\"handleQuery\"\r\n        />\r\n      </el-form-item>\r\n      <el-form-item label=\"经度\" prop=\"longitude\">\r\n        <el-input\r\n          v-model=\"queryParams.longitude\"\r\n          placeholder=\"请输入经度\"\r\n          clearable\r\n          @keyup.enter.native=\"handleQuery\"\r\n        />\r\n      </el-form-item>\r\n      <el-form-item label=\"纬度\" prop=\"latitude\">\r\n        <el-input\r\n          v-model=\"queryParams.latitude\"\r\n          placeholder=\"请输入纬度\"\r\n          clearable\r\n          @keyup.enter.native=\"handleQuery\"\r\n        />\r\n      </el-form-item>\r\n      <el-form-item label=\"风险等级\" prop=\"riskLevel\">\r\n        <el-input\r\n          v-model=\"queryParams.riskLevel\"\r\n          placeholder=\"请输入风险等级\"\r\n          clearable\r\n          @keyup.enter.native=\"handleQuery\"\r\n        />\r\n      </el-form-item>\r\n      <el-form-item label=\"来源\" prop=\"source\">\r\n        <el-input\r\n          v-model=\"queryParams.source\"\r\n          placeholder=\"请输入来源\"\r\n          clearable\r\n          @keyup.enter.native=\"handleQuery\"\r\n        />\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-button type=\"primary\" icon=\"el-icon-search\" size=\"mini\" @click=\"handleQuery\">搜索</el-button>\r\n        <el-button icon=\"el-icon-refresh\" size=\"mini\" @click=\"resetQuery\">重置</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n\r\n    <el-row :gutter=\"10\" class=\"mb8\">\r\n      <el-col :span=\"1.5\">\r\n        <el-button\r\n          type=\"primary\"\r\n          plain\r\n          icon=\"el-icon-plus\"\r\n          size=\"mini\"\r\n          @click=\"handleAdd\"\r\n          v-hasPermi=\"['data:trajectory:add']\"\r\n        >新增</el-button>\r\n      </el-col>\r\n      <el-col :span=\"1.5\">\r\n        <el-button\r\n          type=\"success\"\r\n          plain\r\n          icon=\"el-icon-edit\"\r\n          size=\"mini\"\r\n          :disabled=\"single\"\r\n          @click=\"handleUpdate\"\r\n          v-hasPermi=\"['data:trajectory:edit']\"\r\n        >修改</el-button>\r\n      </el-col>\r\n      <el-col :span=\"1.5\">\r\n        <el-button\r\n          type=\"danger\"\r\n          plain\r\n          icon=\"el-icon-delete\"\r\n          size=\"mini\"\r\n          :disabled=\"multiple\"\r\n          @click=\"handleDelete\"\r\n          v-hasPermi=\"['data:trajectory:remove']\"\r\n        >删除</el-button>\r\n      </el-col>\r\n      <el-col :span=\"1.5\">\r\n        <el-button\r\n          type=\"warning\"\r\n          plain\r\n          icon=\"el-icon-download\"\r\n          size=\"mini\"\r\n          @click=\"handleExport\"\r\n          v-hasPermi=\"['data:trajectory:export']\"\r\n        >导出</el-button>\r\n      </el-col>\r\n      <right-toolbar :showSearch.sync=\"showSearch\" @queryTable=\"getList\"></right-toolbar>\r\n    </el-row>\r\n\r\n    <el-table v-loading=\"loading\" :data=\"trajectoryList\" @selection-change=\"handleSelectionChange\">\r\n      <el-table-column type=\"selection\" width=\"55\" align=\"center\" />\r\n      <el-table-column label=\"轨迹id\" align=\"center\" prop=\"id\" />\r\n      <el-table-column label=\"患者id\" align=\"center\" prop=\"pid\" />\r\n      <el-table-column label=\"起始时间\" align=\"center\" prop=\"startTime\" width=\"180\">\r\n        <template slot-scope=\"scope\">\r\n          <span>{{ parseTime(scope.row.startTime, '{y}-{m}-{d}') }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"结束时间\" align=\"center\" prop=\"endTime\" width=\"180\">\r\n        <template slot-scope=\"scope\">\r\n          <span>{{ parseTime(scope.row.endTime, '{y}-{m}-{d}') }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"场所\" align=\"center\" prop=\"place\" />\r\n      <el-table-column label=\"经度\" align=\"center\" prop=\"longitude\" />\r\n      <el-table-column label=\"纬度\" align=\"center\" prop=\"latitude\" />\r\n      <el-table-column label=\"风险等级\" align=\"center\" prop=\"riskLevel\" />\r\n      <el-table-column label=\"原始文本\" align=\"center\" prop=\"rawText\" />\r\n      <el-table-column label=\"来源\" align=\"center\" prop=\"source\" />\r\n      <el-table-column label=\"操作\" align=\"center\" class-name=\"small-padding fixed-width\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button\r\n            size=\"mini\"\r\n            type=\"text\"\r\n            icon=\"el-icon-edit\"\r\n            @click=\"handleUpdate(scope.row)\"\r\n            v-hasPermi=\"['data:trajectory:edit']\"\r\n          >修改</el-button>\r\n          <el-button\r\n            size=\"mini\"\r\n            type=\"text\"\r\n            icon=\"el-icon-delete\"\r\n            @click=\"handleDelete(scope.row)\"\r\n            v-hasPermi=\"['data:trajectory:remove']\"\r\n          >删除</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    \r\n    <pagination\r\n      v-show=\"total>0\"\r\n      :total=\"total\"\r\n      :page.sync=\"queryParams.pageNum\"\r\n      :limit.sync=\"queryParams.pageSize\"\r\n      @pagination=\"getList\"\r\n    />\r\n\r\n    <!-- 添加或修改轨迹数据对话框 -->\r\n    <el-dialog :title=\"title\" :visible.sync=\"open\" width=\"500px\" append-to-body>\r\n      <el-form ref=\"form\" :model=\"form\" :rules=\"rules\" label-width=\"80px\">\r\n        <el-form-item label=\"患者id\" prop=\"pid\">\r\n          <el-input v-model=\"form.pid\" placeholder=\"请输入患者id\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"起始时间\" prop=\"startTime\">\r\n          <el-date-picker clearable\r\n            v-model=\"form.startTime\"\r\n            type=\"date\"\r\n            value-format=\"yyyy-MM-dd\"\r\n            placeholder=\"请选择起始时间\">\r\n          </el-date-picker>\r\n        </el-form-item>\r\n        <el-form-item label=\"结束时间\" prop=\"endTime\">\r\n          <el-date-picker clearable\r\n            v-model=\"form.endTime\"\r\n            type=\"date\"\r\n            value-format=\"yyyy-MM-dd\"\r\n            placeholder=\"请选择结束时间\">\r\n          </el-date-picker>\r\n        </el-form-item>\r\n        <el-form-item label=\"场所\" prop=\"place\">\r\n          <el-input v-model=\"form.place\" placeholder=\"请输入场所\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"经度\" prop=\"longitude\">\r\n          <el-input v-model=\"form.longitude\" placeholder=\"请输入经度\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"纬度\" prop=\"latitude\">\r\n          <el-input v-model=\"form.latitude\" placeholder=\"请输入纬度\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"风险等级\" prop=\"riskLevel\">\r\n          <el-input v-model=\"form.riskLevel\" placeholder=\"请输入风险等级\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"原始文本\" prop=\"rawText\">\r\n          <el-input v-model=\"form.rawText\" type=\"textarea\" placeholder=\"请输入内容\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"来源\" prop=\"source\">\r\n          <el-input v-model=\"form.source\" placeholder=\"请输入来源\" />\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button type=\"primary\" @click=\"submitForm\">确 定</el-button>\r\n        <el-button @click=\"cancel\">取 消</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { listTrajectory, getTrajectory, delTrajectory, addTrajectory, updateTrajectory } from \"@/api/data/trajectory\";\r\n\r\nexport default {\r\n  name: \"Trajectory\",\r\n  data() {\r\n    return {\r\n      // 遮罩层\r\n      loading: true,\r\n      // 选中数组\r\n      ids: [],\r\n      // 非单个禁用\r\n      single: true,\r\n      // 非多个禁用\r\n      multiple: true,\r\n      // 显示搜索条件\r\n      showSearch: true,\r\n      // 总条数\r\n      total: 0,\r\n      // 轨迹数据表格数据\r\n      trajectoryList: [],\r\n      // 弹出层标题\r\n      title: \"\",\r\n      // 是否显示弹出层\r\n      open: false,\r\n      // 查询参数\r\n      queryParams: {\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        pid: null,\r\n        startTime: null,\r\n        endTime: null,\r\n        place: null,\r\n        longitude: null,\r\n        latitude: null,\r\n        riskLevel: null,\r\n        rawText: null,\r\n        source: null\r\n      },\r\n      // 表单参数\r\n      form: {},\r\n      // 表单校验\r\n      rules: {\r\n      }\r\n    };\r\n  },\r\n  created() {\r\n    this.getList();\r\n  },\r\n  methods: {\r\n    /** 查询轨迹数据列表 */\r\n    getList() {\r\n      this.loading = true;\r\n      listTrajectory(this.queryParams).then(response => {\r\n        this.trajectoryList = response.rows;\r\n        this.total = response.total;\r\n        this.loading = false;\r\n      });\r\n    },\r\n    // 取消按钮\r\n    cancel() {\r\n      this.open = false;\r\n      this.reset();\r\n    },\r\n    // 表单重置\r\n    reset() {\r\n      this.form = {\r\n        id: null,\r\n        pid: null,\r\n        startTime: null,\r\n        endTime: null,\r\n        place: null,\r\n        longitude: null,\r\n        latitude: null,\r\n        riskLevel: null,\r\n        rawText: null,\r\n        source: null\r\n      };\r\n      this.resetForm(\"form\");\r\n    },\r\n    /** 搜索按钮操作 */\r\n    handleQuery() {\r\n      this.queryParams.pageNum = 1;\r\n      this.getList();\r\n    },\r\n    /** 重置按钮操作 */\r\n    resetQuery() {\r\n      this.resetForm(\"queryForm\");\r\n      this.handleQuery();\r\n    },\r\n    // 多选框选中数据\r\n    handleSelectionChange(selection) {\r\n      this.ids = selection.map(item => item.id)\r\n      this.single = selection.length!==1\r\n      this.multiple = !selection.length\r\n    },\r\n    /** 新增按钮操作 */\r\n    handleAdd() {\r\n      this.reset();\r\n      this.open = true;\r\n      this.title = \"添加轨迹数据\";\r\n    },\r\n    /** 修改按钮操作 */\r\n    handleUpdate(row) {\r\n      this.reset();\r\n      const id = row.id || this.ids\r\n      getTrajectory(id).then(response => {\r\n        this.form = response.data;\r\n        this.open = true;\r\n        this.title = \"修改轨迹数据\";\r\n      });\r\n    },\r\n    /** 提交按钮 */\r\n    submitForm() {\r\n      this.$refs[\"form\"].validate(valid => {\r\n        if (valid) {\r\n          if (this.form.id != null) {\r\n            updateTrajectory(this.form).then(response => {\r\n              this.$modal.msgSuccess(\"修改成功\");\r\n              this.open = false;\r\n              this.getList();\r\n            });\r\n          } else {\r\n            addTrajectory(this.form).then(response => {\r\n              this.$modal.msgSuccess(\"新增成功\");\r\n              this.open = false;\r\n              this.getList();\r\n            });\r\n          }\r\n        }\r\n      });\r\n    },\r\n    /** 删除按钮操作 */\r\n    handleDelete(row) {\r\n      const ids = row.id || this.ids;\r\n      this.$modal.confirm('是否确认删除轨迹数据编号为\"' + ids + '\"的数据项？').then(function() {\r\n        return delTrajectory(ids);\r\n      }).then(() => {\r\n        this.getList();\r\n        this.$modal.msgSuccess(\"删除成功\");\r\n      }).catch(() => {});\r\n    },\r\n    /** 导出按钮操作 */\r\n    handleExport() {\r\n      this.download('data/trajectory/export', {\r\n        ...this.queryParams\r\n      }, `trajectory_${new Date().getTime()}.xlsx`)\r\n    }\r\n  }\r\n};\r\n</script>\r\n"]}]}